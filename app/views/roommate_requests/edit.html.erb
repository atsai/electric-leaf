<h1>Editing roommate_request</h1>

<% form_for(@roommate_request) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :requester_id %><br />
    <%= f.text_field :requester_id %>
  </p>
  <p>
    <%= f.label :status %><br />
    <%= f.text_field :status %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @roommate_request %> |
<%= link_to 'Back', roommate_requests_path %>